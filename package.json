{
  "name": "dental-diary",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "build-server": "npm run clean && npm run transpile",
    "server": "node -r dotenv/config ./dist-server/bin/www",
    "dev": "NODE_ENV=development npm run build-server && npm run server",
    "prod": "NODE_ENV=production npm run build-server && npm run server",
    "start": "npm run prod",
    "webdev": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rm -rf dist-server",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "pug": "2.0.0-beta11",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.5",
    "typeface-roboto": "0.0.75",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "file-loader": "^5.1.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
